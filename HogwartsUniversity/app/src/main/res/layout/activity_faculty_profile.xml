                <TextView
                    android:id="@+id/lbl_percentage"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:textSize="18sp"
                    android:textColor="@color/bluebg"
