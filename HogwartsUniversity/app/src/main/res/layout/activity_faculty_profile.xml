                    android:textColor="@color/bluebg" />

                <TextView
                    android:id="@+id/txt_address"
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:textSize="18sp"
                    android:layout_marginLeft="170dp"
