                        android:id="@+id/lbl_edit_address"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:text="Address :"
                        android:textSize="18sp"
                        android:textColor="@color/bluebg" />

