                <TextView
                    android:id="@+id/txt_percentage"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="170dp"
                    android:textSize="18sp"
                    />
            </RelativeLayout>
        </RelativeLayout>

